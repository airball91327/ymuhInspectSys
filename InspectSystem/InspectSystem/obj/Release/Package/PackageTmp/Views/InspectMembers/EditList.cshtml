@model IEnumerable<InspectSystem.Models.InspectMemberAreas>

@{
    ViewBag.Title = "所屬巡檢區域";
}

<h2>所屬巡檢區域</h2>

<table class="table table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MemberId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InspectMembers.MemberName)           
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InspectAreas.AreaName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MemberId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InspectMembers.MemberName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InspectAreas.AreaName)
        </td>
        <td>
            @Html.ActionLink("編輯區域", "Edit", new { id = item.MemberId, areaID = item.AreaId }) |
            @Html.ActionLink("刪除區域", "Delete", new { id = item.MemberId, areaID = item.AreaId })
        </td>
    </tr>
}

</table>
