                                            @if (field != null)
                                            {
                                                if (field.Value == "true")
                                                {
                                                    <span>正常</span>
                                                }
                                                else
                                                {
                                                    <span>異常</span>
                                                }
                                            }
                                        </td>
                                    </tr>
                                </table>
                            </td>
