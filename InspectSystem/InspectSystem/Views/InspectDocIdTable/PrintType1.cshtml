@model InspectSystem.Models.InspectDocPrintVModel

<link rel="stylesheet" type="text/css" href="~/Font-awesome-5.14.0/css/all.min.css">
<script src="~/Font-awesome-5.14.0/js/all.min.js"></script>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
    <style type="text/css">
        .style1 {
            width: 100%;
        }

        .style2 {
            width: 100%;
            font-size: 10pt;
        }

        .style3 {
            height: 30px;
            width: 125px;
        }

        .style4 {
            height: 30px;
            width: 75px;
            text-align: center;
        }

        .style5 {
            text-align: center;
            height: 30px;
        }

        .style6 {
            width: 100%;
        }

        .style7 {
            text-align: center;
            height: 100%;
        }

        @@media print {
