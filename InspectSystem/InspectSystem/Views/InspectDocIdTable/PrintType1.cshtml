                                            width="18%">
                                            @{ field = Model.ShiftDetails3.Where(dtl => dtl.ClassName.Contains("鍋爐用水，柴油、液氧存量紀錄"))
                                                                          .Where(dtl => dtl.ItemName.Contains("鍋爐水表")).ElementAt(0); }
                                            @if (field != null)
                                            {
                                                <span>@field.Value</span>
                                            }
                                        </td>
                                        <td class="style5"
                                            style="border-right-style: solid; border-bottom-style: solid; border-right-width: 2px; border-bottom-width: 2px; border-right-color: #000000; border-bottom-color: #000000; text-align: center"
                                            width="18%">
                                            @{ field = Model.ShiftDetails3.Where(dtl => dtl.ClassName.Contains("鍋爐用水，柴油、液氧存量紀錄"))
                                                                          .Where(dtl => dtl.ItemName.Contains("鍋爐儲油槽")).ElementAt(0); }
