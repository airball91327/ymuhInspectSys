                                            <span class="col">3號機</span>
                                            <span class="col">
                                                @{ field = Model.ShiftDetails3.Where(dtl => dtl.ClassName.Contains("鍋爐運轉狀態"))
                                                                              .Where(dtl => dtl.ItemName.Contains("3號機")).ElementAt(0); }
                                                @if (field != null)
                                                {
                                                    if (field.Value == "true")
                                                    {
                                                        <span>ON</span>
                                                    }
                                                    else
                                                    {
                                                        <span>OFF</span>
                                                    }
                                                }
                                            </span>
                                        </td>
                                    </tr>
                                </table>
                            </td>
